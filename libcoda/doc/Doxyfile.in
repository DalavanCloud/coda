PROJECT_NAME           = coda
PROJECT_NUMBER         = @VERSION@
OUTPUT_DIRECTORY       = @top_builddir@/doc
FULL_PATH_NAMES        = NO
EXTRACT_STATIC         = YES
HIDE_UNDOC_MEMBERS     = YES
INTERNAL_DOCS          = NO
VERBATIM_HEADERS       = NO
HIDE_SCOPE_NAMES       = YES
SHOW_INCLUDE_FILES     = NO
INLINE_INFO            = YES
OPTIMIZE_OUTPUT_FOR_C  = YES
DISTRIBUTE_GROUP_DOC   = YES
QUIET                  = YES
WARN_IF_UNDOCUMENTED   = NO
INPUT                  = @srcdir@/extrapages @top_srcdir@/libcoda @top_builddir@/coda.h
STRIP_FROM_PATH        = @srcdir@ @top_srcdir@/libcoda @top_builddir@
REFERENCED_BY_RELATION = NO
HTML_OUTPUT            = html/libcoda
HTML_FOOTER            = @srcdir@/libcoda_footer.html
HTML_HEADER            = @srcdir@/libcoda_header.html
ENUM_VALUES_PER_LINE   = 1
DISABLE_INDEX          = YES
GENERATE_LATEX         = NO
LATEX_OUTPUT           = latex/libcoda
PDF_HYPERLINKS         = YES
USE_PDFLATEX           = YES
LATEX_BATCHMODE        = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = YES
PREDEFINED             = LIBCODA_API= HAVE_HDF4= HAVE_HDF5=
